[Main]
#ambientLight = (int)(1000 * AmbientLightLevel)
ambientLight=100
#visualMagnitude = (int)(1000 * FaintestVisible)
visualMagnitude=7787
#galaxyLightGain = (int)(1000 * GalaxyLightGain)
galaxyLightGain=0
#Rendering limit in light-years
distanceLimit=1000000
#Display time in terms of local time zone
localTime=false
#Level of Detail in the heads-up-display. 0=None, 1=Terse, 2=Verbose
verbosity=2
#Style of star rendering. 0=Fuzzy Points, 1=Points, 2=Scaled Discs
starStyle=0
#Resolution of textures. 0=Low, 1=Medium, 2=High
textureResolution=1
altSurfaceName=
#Sync Framerate to Video Refresh
videoSync=false

[Window]
width=640
height=480
x=0
y=0
fullScreen=true

#All Render Flag values must be true or false
[RenderFlags]
stars=true
planets=true
galaxies=false
diagrams=false
cloudMaps=true
orbits=true
celestialSphere=false
nightMaps=false
atmospheres=true
smoothLines=true
eclipseShadows=true
ringShadows=true
boundaries=true
autoMag=true
cometTails=true
markers=false
partialTrajectories=false
nebulae=true
openClusters=false
globulars=false
gridGalactic=false
gridEcliptic=false
gridHorizontal=false

#All Orbit Mask values must be true or false
[OrbitMask]
planet=true
moon=true
asteroid=false
comet=false
spacecraft=false
invisible=false
unknown=false

#All Label Mode values must be true or false
[LabelMode]
star=false
planet=true
moon=false
constellation=false
galaxy=false
asteroid=false
spacecraft=true
location=false
comet=false
nebula=false
opencluster=false
i18n=false
globular=false
